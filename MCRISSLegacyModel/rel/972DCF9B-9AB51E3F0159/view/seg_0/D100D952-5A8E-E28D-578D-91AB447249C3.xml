<userDefinedSQL><![CDATA[SELECT 
 '-1' AS ID,  
 STATE_CODE AS Code,  
 STATE_TEXT AS Name 
FROM [dbo].[GEO_LOCATION] WITH (NOLOCK) 
WHERE  
 (ISNUMERIC(STATE_CODE) = 1) AND (STATE_CODE <> '00') AND (COUNTY_CODE IN ('000', '0')) AND (CITY_CODE IN ('0', '0000'))]]></userDefinedSQL>
